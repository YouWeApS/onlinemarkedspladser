<article id="login-view">
  <h1><%= t('.title') %></h1>

  <%= form_for(@user, url: v1_ui_login_path) do |f| %>
    <p>
      <%= f.email_field :email %>
    </p>

    <p>
      <%= f.password_field :password %>
    </p>

    <p>
      <button><%= t('.button') %></button>
    </p>
  <% end %>
</article>
